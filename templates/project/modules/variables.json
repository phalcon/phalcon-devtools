{
    "useConfigIni": {
        "true": {
            "configLoader": "new Ini(APP_PATH  . \"/config/config.ini\")",
            "iniConfigImport": "use Phalcon\\Config\\Adapter\\Ini;"
        },
        "false": {
            "configLoader": "include APP_PATH . \"/config/config.php\"",
            "iniConfigImport": ""
        }
    }
}
